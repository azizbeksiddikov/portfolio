        </section>

        <!-- Contacts Section -->
        <section id="contacts" class="section">
          <div class="contacts-content">
            <!-- Social Links -->
            <div class="social-links">
              <a
                href="https://github.com/azizbeksiddikov"
                class="social-icon"
                target="_blank"
              >
                <img class="icon_image" src="/images/github.jpg" alt="GitHub" />
              </a>
              <a
                href="https://www.linkedin.com/in/azzd/"
                class="social-icon"
                target="_blank"
              >
                <img
                  class="icon_image"
                  src="/images/linkedin.png"
                  alt="LinkedIn"
                />
              </a>
              <a href="mailto:azbeksid@gmail.com" class="social-icon">
                <img class="icon_image" src="/images/email.jpg" alt="Email" />
              </a>
            </div>

            <form id="contact-form" class="contact-form">
              <input type="text" name="name" placeholder="Your Name" required />
              <input
                type="email"
                name="email"
                placeholder="Your Email"
                required
              />
              <textarea
                name="message"
                placeholder="Your Message"
                required
              ></textarea>
              <button type="submit">Send Message</button>
            </form>
